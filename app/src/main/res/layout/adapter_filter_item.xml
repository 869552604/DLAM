<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:paddingLeft="8dp"
    android:paddingRight="8dp"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <TextView
        android:id="@+id/tv_code"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintLeft_toLeftOf="parent"
        android:textColor="@color/tv_color"
        android:paddingTop="8dp"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/tv_name"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_code"
        app:layout_constraintBottom_toBottomOf="@id/tv_code"
        app:layout_constraintLeft_toRightOf="@id/tv_code"
        android:paddingTop="8dp"
        android:textColor="@color/tv_color"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:id="@+id/tv_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_code"
        android:paddingTop="8dp"
        app:layout_constraintBottom_toBottomOf="@id/tv_code"
        app:layout_constraintLeft_toRightOf="@id/tv_name"
        android:textColor="@color/tv_color"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:id="@+id/tv_tun"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="8dp"
        android:textColor="@color/tv_color"
        app:layout_constraintTop_toBottomOf="@id/tv_code"
        app:layout_constraintLeft_toLeftOf="@id/tv_code"/>

    <TextView
        android:id="@+id/tv_quantity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_tun"
        app:layout_constraintBottom_toBottomOf="@id/tv_tun"
        app:layout_constraintLeft_toRightOf="@id/tv_tun"
        android:textColor="@color/tv_color"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:id="@+id/tv_change"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_tun"
        app:layout_constraintLeft_toLeftOf="@id/tv_code"
        android:textColor="@color/tv_color"
        android:layout_marginTop="8dp"
        />

    <TextView
        android:id="@+id/tv_amplitude"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_change"
        app:layout_constraintBottom_toBottomOf="@id/tv_change"
        app:layout_constraintLeft_toRightOf="@id/tv_change"
        android:textColor="@color/tv_color"
        android:layout_marginLeft="8dp"/>



    <TextView
        android:id="@+id/tv_all_price"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_change"
        app:layout_constraintBottom_toBottomOf="@id/tv_change"
        app:layout_constraintLeft_toRightOf="@id/tv_amplitude"
        android:textColor="@color/tv_color"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:id="@+id/tv_max"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_change"
        app:layout_constraintLeft_toLeftOf="@id/tv_code"
        android:textColor="@color/tv_color"
        android:layout_marginTop="8dp"
        />

    <TextView
        android:id="@+id/tv_min"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="@id/tv_max"
        app:layout_constraintBottom_toBottomOf="@id/tv_max"
        app:layout_constraintLeft_toRightOf="@id/tv_max"
        android:textColor="@color/tv_color"
        android:layout_marginLeft="8dp"/>

    <TextView
        android:id="@+id/tv_info1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_max"
        app:layout_constraintLeft_toLeftOf="@id/tv_code"
        android:textColor="@color/tv_color"
        android:layout_marginTop="8dp"
        />

    <TextView
        android:id="@+id/tv_info2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@id/tv_info1"
        app:layout_constraintLeft_toLeftOf="@id/tv_code"
        android:textColor="@color/tv_color"
        android:layout_marginTop="8dp"
        />

    <View
        android:id="@+id/bottom_line"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_marginTop="8dp"
        android:background="#999"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toBottomOf="@id/tv_info2" />

    <TextView
        android:id="@+id/fenshi"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:background="@drawable/button_normal"
        android:gravity="center"
        android:padding="5dp"
        android:text="看盘"
        android:textColor="#000"
        app:layout_constraintBottom_toTopOf="@id/rixian"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/rixian"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:background="@drawable/button_normal"
        android:gravity="center"
        android:padding="5dp"
        android:text="日线"
        android:textColor="#000"
        app:layout_constraintTop_toBottomOf="@id/fenshi"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintBottom_toTopOf="@+id/kxian" />

    <TextView
        android:id="@+id/kxian"
        android:layout_width="60dp"
        android:layout_height="wrap_content"
        android:layout_marginRight="10dp"
        android:background="@drawable/button_normal"
        android:gravity="center"
        android:padding="5dp"
        android:text="线图"
        android:textColor="#000"
        app:layout_constraintBottom_toBottomOf="@+id/bottom_line"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toBottomOf="@id/rixian" />

</androidx.constraintlayout.widget.ConstraintLayout>
